select ename,sal from emp where sal between 1000 and 3000;
select ename,sal from emp where ename between 'a' and 'l';
select * from dept where deptno between 10 and 30;
select * from emp where comm between 500 and 1000;
select * from emp where comm is null;
select * from emp where comm is not null;
select * from emp where mgr is null;
select * from emp where mgr is not null;
select * from emp where ename in ('smith','allen','scott');
select ename,sal from emp where deptno in (10,20);
select * from dept where dname in('research','sales'); 
select empno,ename,comm,job from emp where job in ('salesman','managers'); 
select ename name,job designation,hiredate joiningdate from emp;
select ename as name,job as  designation,hiredate as  joiningdate from emp;
select empno as empid, sal as salary,comm as commission from emp;
