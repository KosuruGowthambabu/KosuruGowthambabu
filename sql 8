select deptno from emp union select deptno from dept;
select deptno from emp union all select deptno from dept;
select * from emp where deptno =30 and sal>1500;
select * from emp where deptno =10 and mgr is not null ;
select * from emp where ename between 'b' and 'r';
select * from emp where comm is null and job = 'clerk';
select ename,sal,deptno from emp where deptno in ( 10,  30) and mgr is null;
select ename,sal*12 from emp where deptno = 30;
select * from  emp where ename like 'a%' or 'e%' or 'i%' or 'o%' or 'u%';
select * from emp where job = 'salesman' and ename like '%e' and sal>1000;
select* from emp where deptno =20 and sal between 3000 and 5000;
select * from emp where hiredate > '1981-12-31';
select * from emp where hiredate between '1981-02-01' and '1981-02-28';
select '* * *' from dual union all
select ' * *' from dual union all
select    '  *  ' from dual;
